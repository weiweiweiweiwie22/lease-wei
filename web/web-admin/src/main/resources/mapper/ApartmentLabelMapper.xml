<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.weiwei.lease.web.admin.mapper.ApartmentLabelMapper">

    <select id="selectListById" resultType="com.weiwei.lease.model.entity.LabelInfo">
        select
            id,
            type,
            name
        from label_info
        where is_deleted = 0
        and id in (select
                       label_id
                   from apartment_label
                   where apartment_id = #{id}
                     and is_deleted = 0)

    </select>
</mapper>
